{
  "name": "@progress/kendo-charts",
  "description": "Kendo UI platform-independent Charts library",
  "author": "Progress",
  "license": "SEE LICENSE IN license.txt",
  "version": "2.7.2",
  "main": "dist/npm/main.js",
  "module": "dist/es/main.js",
  "jsnext:main": "dist/es/main.js",
  "es2015": "dist/es2015/main.js",
  "typings": "dist/npm/main.d.ts",
  "jsdelivr": "dist/cdn/main.js",
  "unpkg": "dist/cdn/main.js",
  "sideEffects": true,
  "scripts": {
    "start": "gulp start",
    "build": "gulp build-rollup-package build-cdn",
    "test": "cross-env TZ=Europe/Sofia jest --watchAll",
    "test:ci": "cross-env TZ=Europe/Sofia jest",
    "e2e": "cross-env TZ=Europe/Sofia ENV_BROWSER=Chrome_headless gulp watch-e2e",
    "e2e:ci": "cross-env TZ=Europe/Sofia CHROME_BIN=`which chrome || which chromium-browser` ENV_BROWSER=Chrome_headless gulp e2e",
    "e2e-visual:ci": "./bin/run-e2e-visual.sh",
    "lint": "eslint .",
    "prepare": "husky"
  },
  "keywords": [
    "Kendo UI",
    "Progress",
    "DataViz",
    "Angular 2",
    "Charts"
  ],
  "peerDependencies": {
    "@progress/kendo-drawing": "^1.21.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@progress/kendo-drawing": "^1.21.0",
    "@progress/kendo-e2e": "^0.6.0",
    "@progress/kendo-file-saver": "^1.1.1",
    "@progress/kendo-intl": "^3.1.1",
    "@progress/kendo-package-tasks": "^5.0.0",
    "@progress/kendo-svg-icons": "^1.5.0",
    "@progress/kendo-theme-default": "^6.3.0",
    "@progress/pako-esm": "^1.0.1",
    "@types/jest": "^26.0.24",
    "cross-env": "^7.0.3",
    "d3-scale-chromatic": "^3.0.0",
    "es-jest": "^2.1.0",
    "eslint": "^8.10.0",
    "gulp": "^5.0.0",
    "husky": "^7.0.1",
    "jest-cli": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "semantic-release": "^24.2.3"
  },
  "overrides": {
    "esbuild": "~0.25.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/telerik/kendo-charts.git"
  },
  "bugs": {
    "url": "https://github.com/telerik/kendo-angular"
  },
  "files": [
    "dist"
  ]
